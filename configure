  fi



if $sim_ac_make_dspex; then

if test x"${sim_ac_configuration_settings+set}" = x"set"; then
  sim_ac_configuration_settings="$sim_ac_configuration_settings|So${Gui}Examples build type:msvc .dsp"
else
  sim_ac_configuration_settings="So${Gui}Examples build type:msvc .dsp"
fi


  CC=$soguiexamples_src_dir/cfg/gendspex.sh
  CXX=$soguiexamples_src_dir/cfg/gendspex.sh
  CXXLD=$soguiexamples_src_dir/cfg/gendspex.sh

  # Notes:
  # - these defines are picked up by gendspex.sh
  # - template basename = vc6 -> workspace_template_vc6.txt, etc
  # - $(DEFAULT_INCLUDES) and $(INCLUDES) are set by the generated
